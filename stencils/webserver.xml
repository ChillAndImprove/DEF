<shapes name="mxgraph.compute">

<shape name="webserver" aspect="fixed" w="190" h="190" strokewidth="1">
    <connections/>
    <foreground>
        <!-- 1. Background Rectangle -->
        <save/>
        <strokecolor color="#85C1E9"/>
        <fillcolor color="#D6EAF8"/>
        <strokewidth width="5"/>
        <roundrect x="0" y="0" w="190" h="190" arcsize="13"/>
        <fillstroke/>
        <restore/>

        <!-- 2. Server Base -->
        <save/>
        <strokecolor color="#34495E"/>
        <fillcolor color="#5D6D7E"/>
        <strokewidth width="3"/>
        <rect x="40" y="130" w="110" h="25"/>
        <fillstroke/>
        <restore/>

        <!-- 3. Server Tower -->
        <save/>
        <strokecolor color="#2C3E50"/>
        <fillcolor color="#34495E"/>
        <strokewidth width="2"/>
        <rect x="70" y="60" w="50" h="70"/>
        <fillstroke/>
        <restore/>

        <!-- 4. Web/HTTP Indicators -->
        <save/>
        <strokecolor color="#E74C3C"/>
        <fillcolor color="#E74C3C"/>
        <strokewidth width="1"/>
        <!-- HTTP indicator dots -->
        <ellipse x="80" y="70" w="8" h="8"/>
        <fillstroke/>
        <ellipse x="102" y="70" w="8" h="8"/>
        <fillstroke/>

        <!-- Server rack lines -->
        <strokecolor color="#BDC3C7"/>
        <strokewidth width="2"/>
        <path>
            <move x="75" y="85"/>
            <line x="115" y="85"/>
        </path>
        <stroke/>
        <path>
            <move x="75" y="95"/>
            <line x="115" y="95"/>
        </path>
        <stroke/>
        <path>
            <move x="75" y="105"/>
            <line x="115" y="105"/>
        </path>
        <stroke/>
        <restore/>

    </foreground>
</shape>

</shapes>